{
  "author": "Nate Wienert",
  "name": "reactor",
  "description": "A React Isomorphic App",
  "version": "0.3.0",
  "main": "./app/app.jsx",
  "scripts": {
    "install": "npm install -g node-dev && npm install && bower install && mkdir build",
    "test": "mocha -R spec tests/**-test.js",
    "test-watch": "mocha -G -w -R min tests/**-test.js",
    "dev-server": "node server.js --config config.dev.js --dev --progress --colors",
    "hot-server": "node server.js --config config.hot.js --dev --hot --progress --colors",
    "iso-server": "node server.js --debug",
    "build": "webpack --config webpack/config.production.js --progress --profile --colors --debug --display-error-details --verbose --display-modules",
    "start": "node server.js"
  },
  "dependencies": {
    "react": "^0.11.2",
    "react-router": "git+https://github.com/rackt/react-router.git#async-props",
    "react-style": "git+https://github.com/js-next/react-style.git",
    "scroller": "^0.0.2",
    "rebound": "^0.0.8",
    "lodash-node": "^2.4.1",
    "react-document-title": "^0.1.1",
    "fluxxor": "^1.5.0",
    "react-ago-component": "^0.5.0",
    "when": "^3.5.0",
    "debug": "^2.1.0",
    "rest": "^1.2.0",
    "immstruct": "^0.9.0",
    "omniscient": "^1.3.0",
    "parseurl": "~1.3.0",
    "object-assign": "~1.0.0"
  },
  "devDependencies": {
    "transform-loader": "^0.2.1",
    "react-hot-loader": "^0.5.0",
    "react-style-webpack-plugin": "git+https://github.com/js-next/react-style-webpack-plugin",
    "extract-text-webpack-plugin": "git+https://github.com/webpack/extract-text-webpack-plugin",
    "webpack": "^1.4.8",
    "style-loader": "^0.6.4",
    "stylus-loader": "^0.4.0",
    "webpack-dev-server": "^1.6.5",
    "css-loader": "^0.7.0",
    "html-loader": "^0.2.2",
    "jsx-loader": "^0.11.2",
    "null-loader": "^0.1.0",
    "react-proxy-loader": "^0.2.2",
    "json-loader": "^0.5.1",
    "chai": "^1.9.2",
    "mocha": "^1.21.5",
    "jsdom": "^1.0.1",
    "expect.js": "0.1",
    "yargs": "^1.3.1",
    "mach": "^0.12.0",
    "exports-loader": "^0.6.2",
    "prepend-file": "^0.1.2",
    "bundle-loader": "^0.5.1",
    "postcss-loader": "^0.1.0",
    "autoprefixer-core": "^3.1.2"
  }
}
